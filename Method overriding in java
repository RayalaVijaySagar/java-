public class methodOverriding{
    public static void main(String[] args)  
    {
        B obj=new B();
        obj.show();
    }
}
class A
{
    public void show()
    {
        System.out.println("in a show");
    }
}
class B extends A
{
   public void show()
   {
       System.out.println("in b show");
   }
}
