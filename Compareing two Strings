 public static void main(String[] args) throws Exception
    {
        String str1="abcdef";
        String str2="efghij";
        for(int i=0;i<6;i++)
        {
           
            char s1=str1.charAt(i);
            for(int j=0;j<6;j++)
            {
                char s2=str2.charAt(j);
                if(s2==s1)
                {
                    System.out.println(str1.charAt(i));
                }
                         
            }
            
        }
    }
