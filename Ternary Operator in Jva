public class JavaApplication16
{

    public static void main(String[] args) 
    {
        int n=10;
        
        int result=0;
        String str="";
        str=(n==10)?"hi":"bye";
        result= (n%2==0) ?10:20;
        System.out.println(result);
        System.out.println(str);
        
        
    }
 }
 
